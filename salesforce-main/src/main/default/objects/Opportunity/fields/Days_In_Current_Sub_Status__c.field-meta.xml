<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_In_Current_Sub_Status__c</fullName>
    <description>The number of days since either the status or sub-status was changed</description>
    <externalId>false</externalId>
    <formula>TODAY() - BLANKVALUE(Last_Status_Change_Date__c, TODAY())</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The number of days since either the status or sub-status was changed</inlineHelpText>
    <label>Days In Current Sub-Status</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
