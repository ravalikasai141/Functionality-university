<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Website_field</fullName>
    <active>true</active>
    <errorConditionFormula>LEN( Website ) &gt; 120</errorConditionFormula>
    <errorDisplayField>Website</errorDisplayField>
    <errorMessage>There cannot be more then 120 characters in the Website field.</errorMessage>
</ValidationRule>
