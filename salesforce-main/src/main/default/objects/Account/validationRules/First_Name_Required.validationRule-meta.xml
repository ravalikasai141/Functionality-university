<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>First_Name_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(RecordType.DeveloperName = &apos;Student_Account&apos;, FirstName = null, $User.LastName &lt;&gt; &quot;bpp_dndirector&quot;)</errorConditionFormula>
    <errorDisplayField>FirstName</errorDisplayField>
    <errorMessage>You must enter a value</errorMessage>
</ValidationRule>
