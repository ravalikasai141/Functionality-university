<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BPP_Delivery_Line_Total_Price__c</fullName>
    <description>Related Order Product List Price, minus the Discount Amount. Then multiplied by Delivery Quantity</description>
    <externalId>false</externalId>
    <formula>(Order_Product__r.ListPrice - Order_Product__r.BPP_Discount_Amount__c) * Quantity__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Delivery Line Total Price</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
