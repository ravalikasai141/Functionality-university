<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Active_TrainingManager</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISBLANK( PS_Profile_Services_ID__c ), Active__c = true)</errorConditionFormula>
