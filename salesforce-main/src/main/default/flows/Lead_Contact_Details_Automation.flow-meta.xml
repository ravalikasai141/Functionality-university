    <description>Ensure that the correct preferences are populating the master fields + mobile numbers are copied to the Mobile field.</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>OR(
  AND(
  ISNEW() ,
    OR(
    NOT(ISBLANK(TEXT([Lead].Preferred_Email__c))) ,
    NOT(ISBLANK(TEXT([Lead].Preferred_Phone__c)))
  )) ,
  OR(
  ISCHANGED([Lead].Preferred_Email__c) ,
  ISCHANGED([Lead].Personal_Email__c ) ,
  ISCHANGED([Lead].Work_Email__c) ,
  ISCHANGED([Lead].Other_Email__c) ,
  ISCHANGED([Lead].Preferred_Phone__c) ,
  ISCHANGED([Lead].Home_Phone__c) ,
  ISCHANGED([Lead].MobilePhone) ,
  ISCHANGED([Lead].Work_Phone__c)
  )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>OR(
  AND(
  ISNEW() ,
    OR(
    NOT(ISBLANK(TEXT({!myVariable_current.Preferred_Email__c}))) ,
    NOT(ISBLANK(TEXT({!myVariable_current.Preferred_Phone__c})))
  )) ,
  OR(
  ISCHANGED({!myVariable_current.Preferred_Email__c}) ,
  ISCHANGED({!myVariable_current.Personal_Email__c} ) ,
  ISCHANGED({!myVariable_current.Work_Email__c}) ,
  ISCHANGED({!myVariable_current.Other_Email__c}) ,
  ISCHANGED({!myVariable_current.Preferred_Phone__c}) ,
  ISCHANGED({!myVariable_current.Home_Phone__c}) ,
  ISCHANGED({!myVariable_current.MobilePhone}) ,
  ISCHANGED({!myVariable_current.Work_Phone__c})
  )
)</expression>
    </formulas>
