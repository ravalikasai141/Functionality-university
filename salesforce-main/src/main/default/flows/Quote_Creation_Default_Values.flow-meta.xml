    <description>Updates BillingAddress and ShippingAddress on Quotes during creation</description>
    <environments>Default</environments>
    <formulas>
        <name>AccountBillingType</name>
        <dataType>String</dataType>
        <expression>IF (TEXT({!$Record.SBQQ__Account__r.Billing_Type__c}) == &quot;In Advance&quot;, &quot;In Advance&quot;, &quot;In Arrears&quot;)</expression>
    </formulas>
    <formulas>
        <description>Concatenates Primary Billing Address Street 1, Address Street 2 and Address Street 3 fields</description>
        <name>PrimaryBillingStreet</name>
        <dataType>String</dataType>
        <expression>{!Get_Primary_Billing_Address.Street_1__c} &amp; BR() &amp; {!Get_Primary_Billing_Address.Street_2__c} &amp; BR() &amp;{!Get_Primary_Billing_Address.Street_3__c}</expression>
    </formulas>
    <formulas>
        <description>Concatenates Primary Delivery Address Street 1, Address Street 2 and Address Street 3 fields</description>
        <name>PrimaryDeliveryStreet</name>
        <dataType>String</dataType>
        <expression>{!Get_Primary_Delivery_Address.Street_1__c} &amp; BR() &amp; {!Get_Primary_Delivery_Address.Street_2__c} &amp; BR() &amp;{!Get_Primary_Delivery_Address.Street_3__c}</expression>
    </formulas>
