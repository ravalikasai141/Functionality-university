    <collectionProcessors>
        <description>Closed opportunities related to the account that meet the conditions</description>
        <name>Closed_Opportunities</name>
        <elementSubtype>FilterCollectionProcessor</elementSubtype>
        <label>Closed Opportunities</label>
        <locationX>446</locationX>
        <locationY>1187</locationY>
        <assignNextValueToReference>currentItem_Closed_Opportunities</assignNextValueToReference>
        <collectionProcessorType>FilterCollectionProcessor</collectionProcessorType>
        <collectionReference>Account_Opportunities</collectionReference>
        <conditionLogic>and</conditionLogic>
        <conditions>
            <leftValueReference>currentItem_Closed_Opportunities.IsClosed</leftValueReference>
            <operator>EqualTo</operator>
            <rightValue>
                <booleanValue>true</booleanValue>
            </rightValue>
        </conditions>
        <conditions>
            <leftValueReference>currentItem_Closed_Opportunities.CloseDate</leftValueReference>
            <operator>GreaterThan</operator>
            <rightValue>
                <elementReference>fxDateOneYearAgo</elementReference>
            </rightValue>
        </conditions>
        <connector>
            <targetReference>Set_Closed_Opportunity_Collection_Count</targetReference>
        </connector>
    </collectionProcessors>
