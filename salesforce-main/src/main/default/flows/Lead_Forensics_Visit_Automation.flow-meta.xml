    <description>Lead Forensics Visit updates (context: after creation/update)</description>
    <environments>Default</environments>
    <formulas>
        <description>Calculate the date one year ago today</description>
        <name>fxDateOneYearAgo</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} - 365</expression>
    </formulas>
    <formulas>
        <description>Calculate how long ago last notification was sent</description>
        <name>fxDaysSinceLastTask</name>
        <dataType>Number</dataType>
        <expression>IF({!Lead} = True, 
{!$Flow.CurrentDate} - {!$Record.LF__Lead__r.Last_Lead_Forensics_Action_Date__c}, 
{!$Flow.CurrentDate} - {!$Record.LF__Account__r.Last_Lead_Forensics_Action_Date__c})</expression>
        <scale>0</scale>
    </formulas>
    <interviewLabel>Lead Forensics Visit Automation {!$Flow.CurrentDateTime}</interviewLabel>
