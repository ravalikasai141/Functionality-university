    <decisions>
        <description>Does the visit require a follow up task to be created? If a task has already been created within a recent set period then another one should not be created.</description>
        <name>Account_Task_Required</name>
        <label>Account Task Required?</label>
        <locationX>545</locationX>
        <locationY>539</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Account_Task_Is_Required</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>fxDaysSinceLastTask</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>30.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fxDaysSinceLastTask</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Account_Opportunities</targetReference>
            </connector>
            <label>Account Task Is Required</label>
        </rules>
    </decisions>
