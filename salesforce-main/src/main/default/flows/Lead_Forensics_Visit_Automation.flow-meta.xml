    <decisions>
        <description>Is the lead active in the current cycle (Establishing Contact / Engaged)?</description>
        <name>Is_Lead_Active_In_Cycle</name>
        <label>Is Lead Active In Cycle?</label>
        <locationX>1964</locationX>
        <locationY>539</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Active_Lead</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.LF__Lead__r.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Establishing Contact</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.LF__Lead__r.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Engaged</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Pipeline_Lead_Task_Details</targetReference>
            </connector>
            <label>Active Lead</label>
        </rules>
        <rules>
            <name>Inactive_Lead</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.LF__Lead__r.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Nurturing</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.LF__Lead__r.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>No Further Action</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Re_engage_Lead</targetReference>
            </connector>
            <label>Inactive Lead</label>
        </rules>
        <rules>
            <name>New_Lead</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.LF__Lead__r.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>New</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_New_Lead_Task_Details</targetReference>
            </connector>
            <label>New Lead</label>
        </rules>
    </decisions>
    <decisions>
        <description>Does the visit require a follow up task to be created? If a task has already been created within a recent set period then another one should not be created.</description>
        <name>Lead_Task_Required</name>
        <label>Lead Task Required?</label>
        <locationX>1502</locationX>
        <locationY>755</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
