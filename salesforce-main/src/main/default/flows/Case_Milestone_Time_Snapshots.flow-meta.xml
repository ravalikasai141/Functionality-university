    <decisions>
        <description>Decision node to determine the nature of the update</description>
        <name>Case_Update_Decision</name>
        <label>Case Update Decision</label>
        <locationX>578</locationX>
        <locationY>287</locationY>
        <defaultConnectorLabel>No Change To Owner Or Status</defaultConnectorLabel>
        <rules>
            <name>Has_The_Status_Changed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
